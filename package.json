{
  "name": "n8n-nodes-gls",
  "version": "0.1.0",
  "description": "N8N Community Nodes Package for GLS ParcelShop Management API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/GLS-IT-Service/n8n-nodes-gls",
  "author": {
    "name": "GLS IT Service",
    "email": "xbp-psm-n8n-community-support@gls-group.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GLS-IT-Service/n8n-nodes-gls.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PsmApi.credentials.js",
      "dist/credentials/PsmAccessTokenApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Gls/Gls.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^5.0.1",
    "prettier": "3.8.0",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
